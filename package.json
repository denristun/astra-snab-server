{
  "name": "astra-snab-server",
  "version": "1.0.0",
  "description": "BackEnd server for astra-snab",
  "main": "server.ts",
  "scripts": {
    "start": "ts-node ./src/server.ts",
    "start:watch": "nodemon",
    "build": "tsc"
  },
  "keywords": [
    "astra-snab"
  ],
  "author": "Denis Mehtahudinov <denristun@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.7",
    "@types/mongoose": "^5.7.32",
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "mongoose": "^5.9.25",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  },
  "devDependencies": {
    "@types/node": "^14.0.26",
    "eslint": "^7.5.0",
    "eslint-plugin-import": "^2.22.0",
    "nodemon": "^2.0.4",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm start",
    "ext": "ts"
  }
}
